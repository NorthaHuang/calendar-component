{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@src": "./src",
          "@components": "./src/components",
          "@helpers": "./src/helpers",
          "@styles": "./src/styles",
          "@type": "./src/types",
          "@public": "./public"
        }
      }
    ]
  ]
}
